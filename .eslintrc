{
  "extends": "standard",
  "rules": {
    "no-unused-expressions": 0,
    "chai-friendly/no-unused-expressions": 2,
    "eqeqeq": "warn",
    "no-throw-literal": "off",
    "handle-callback-err": "warn",
    "no-undef": "warn",
    "no-use-before-define": 0,
    "angular/avoid-scope-typos": 2,
    "angular/module-getter": 2,
    "angular/module-setter": 2,
    "angular/no-private-call": [2,{"allow":["$$route"]}],
    "angular/controller-as-route": 2,
    "angular/controller-as-vm": 2,
    "angular/empty-controller": 2,
    "angular/no-inline-template": [2,{"allowSimple":true}],
    "angular/on-destroy": 2,
    "angular/no-http-callback": 2,
    "angular/no-directive-replace": 2,
    "angular/no-cookiestore": 2,
    "angular/controller-name": [2,"/[A-Z].*Ctrl/"],
    "angular/dumb-inject": 2,
    "angular/function-type": [2,"anonymous"],
    "angular/no-service-method": 2,
    "angular/one-dependency-per-line": 2,
    "angular/rest-service": [2,"$http"],
    "angular/watchers-execution": [2,"$apply"],
    "angular/angularelement": 2,
    "angular/definedundefined": 2,
    "angular/document-service": 2,
    "angular/foreach": 2,
    "angular/interval-service": 2,
    "angular/json-functions": 2,
    "angular/log": 2,
    "angular/no-jquery-angularelement": 2,
    "angular/timeout-service": 2,
    "angular/typecheck-array": 2,
    "angular/typecheck-date": 2,
    "angular/typecheck-function": 2,
    "angular/typecheck-number": 2,
    "angular/typecheck-object": 2,
    "angular/typecheck-string": 2,
    "angular/window-service": 2,
    "angular/di-unused": 2
  },
  "overrides": [
    {
      "files": ["app/**/*.js","config/**/*.js","specs/**/*.js","app.js"],
      "rules": {
        "angular/avoid-scope-typos": 0,
        "angular/module-getter": 0,
        "angular/module-setter": 0,
        "angular/no-private-call": 0,
        "angular/controller-as-route": 0,
        "angular/controller-as-vm": 0,
        "angular/empty-controller": 0,
        "angular/no-inline-template": 0,
        "angular/on-destroy": 0,
        "angular/no-http-callback": 0,
        "angular/no-directive-replace": 0,
        "angular/no-cookiestore": 0,
        "angular/controller-name": 0,
        "angular/dumb-inject": 0,
        "angular/function-type": 0,
        "angular/no-service-method": 0,
        "angular/one-dependency-per-line": 0,
        "angular/rest-service": 0,
        "angular/watchers-execution": 0,
        "angular/angularelement": 0,
        "angular/definedundefined": 0,
        "angular/document-service": 0,
        "angular/foreach": 0,
        "angular/interval-service": 0,
        "angular/json-functions": 0,
        "angular/log": 0,
        "angular/no-jquery-angularelement": 0,
        "angular/timeout-service": 0,
        "angular/typecheck-array": 0,
        "angular/typecheck-date": 0,
        "angular/typecheck-function": 0,
        "angular/typecheck-number": 0,
        "angular/typecheck-object": 0,
        "angular/typecheck-string": 0,
        "angular/window-service": 0,
        "angular/di-unused": 0
      }
    }
  ],
  "globals": {
    "angular": "readonly",
    "io": "readonly",
    "XLSX": "readonly",
    "FileReader": "readonly",
    "StringMask": "readonly"
  },
  "env": {
    "mocha": true
  },
  "plugins": [
        "chai-friendly",
        "angular"
  ]
}
